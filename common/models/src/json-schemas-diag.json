{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Patch",
  "definitions": {
    "Patch": {
      "type": "object",
      "properties": {
        "_id": {
          "type": "string"
        },
        "appName": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "status": {
          "type": "string",
          "enum": [
            "completed",
            "failed"
          ]
        },
        "startDate": {
          "type": "string",
          "format": "date-time"
        },
        "finishDate": {
          "type": "string",
          "format": "date-time"
        },
        "error": {},
        "errors": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "message": {
                "type": "string"
              },
              "stack": {
                "type": "string"
              }
            },
            "required": [
              "message",
              "stack"
            ],
            "additionalProperties": false
          }
        },
        "warns": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "updates": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "_id",
        "appName",
        "name",
        "description",
        "status",
        "startDate",
        "finishDate"
      ],
      "additionalProperties": false
    }
  }
}