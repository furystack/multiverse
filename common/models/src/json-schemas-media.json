{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "EncodingTask": {
      "additionalProperties": false,
      "properties": {
        "_id": {
          "type": "string"
        },
        "authToken": {
          "type": "string"
        },
        "creationDate": {
          "description": "The task creation date",
          "format": "date-time",
          "type": "string"
        },
        "error": {
        },
        "finishDate": {
          "format": "date-time",
          "type": "string"
        },
        "log": {
          "items": {
            "$ref": "#/definitions/LeveledLogEntry%3Cany%3E"
          },
          "type": "array"
        },
        "mediaInfo": {
          "additionalProperties": false,
          "description": "Generic info about the media to encode",
          "properties": {
            "library": {
              "$ref": "#/definitions/MovieLibrary",
              "description": "The related movie library"
            },
            "movie": {
              "$ref": "#/definitions/Movie",
              "description": "The Movie entity"
            }
          },
          "required": [
            "movie",
            "library"
          ],
          "type": "object"
        },
        "modificationDate": {
          "format": "date-time",
          "type": "string"
        },
        "percent": {
          "description": "Shows how the progress of the encoding goes, reported by the worker agent",
          "type": "number"
        },
        "startDate": {
          "format": "date-time",
          "type": "string"
        },
        "status": {
          "description": "The current status of the task",
          "enum": [
            "pending",
            "inProgress",
            "finished",
            "failed",
            "cancelled"
          ],
          "type": "string"
        },
        "workerInfo": {
          "additionalProperties": false,
          "description": "Generic info about the worker who works on the task",
          "properties": {
            "ip": {
              "description": "The IP address",
              "type": "string"
            }
          },
          "required": [
            "ip"
          ],
          "type": "object"
        }
      },
      "required": [
        "_id",
        "authToken",
        "mediaInfo",
        "status",
        "percent",
        "creationDate",
        "modificationDate"
      ],
      "type": "object"
    },
    "EncodingType": {
      "anyOf": [
        {
          "$ref": "#/definitions/Vp9EncodingType"
        },
        {
          "$ref": "#/definitions/X264EncodingType"
        }
      ]
    },
    "FailedOmdbMetadata": {
      "additionalProperties": false,
      "properties": {
        "Error": {
        },
        "Response": {
          "const": "False",
          "type": "string"
        }
      },
      "required": [
        "Response",
        "Error"
      ],
      "type": "object"
    },
    "FetchedOmdbMetadata": {
      "additionalProperties": false,
      "properties": {
        "Actors": {
          "type": "string"
        },
        "Awards": {
          "type": "string"
        },
        "BoxOffice": {
          "type": "string"
        },
        "Country": {
          "type": "string"
        },
        "DVD": {
          "type": "string"
        },
        "Director": {
          "type": "string"
        },
        "Episode": {
          "type": "string"
        },
        "Genre": {
          "type": "string"
        },
        "Language": {
          "type": "string"
        },
        "Metascore": {
          "type": "string"
        },
        "Plot": {
          "type": "string"
        },
        "Poster": {
          "type": "string"
        },
        "Production": {
          "type": "string"
        },
        "Rated": {
          "type": "string"
        },
        "Ratings": {
          "items": {
            "additionalProperties": false,
            "properties": {
              "Source": {
                "type": "string"
              },
              "Value": {
                "type": "string"
              }
            },
            "required": [
              "Source",
              "Value"
            ],
            "type": "object"
          },
          "type": "array"
        },
        "Released": {
          "type": "string"
        },
        "Response": {
          "const": "True",
          "type": "string"
        },
        "Runtime": {
          "type": "string"
        },
        "Season": {
          "type": "string"
        },
        "Title": {
          "type": "string"
        },
        "Type": {
          "enum": [
            "movie",
            "series"
          ],
          "type": "string"
        },
        "Website": {
          "type": "string"
        },
        "Writer": {
          "type": "string"
        },
        "Year": {
          "type": "string"
        },
        "imdbID": {
          "type": "string"
        },
        "imdbRating": {
          "type": "string"
        },
        "imdbVotes": {
          "type": "string"
        },
        "seriesID": {
          "type": "string"
        }
      },
      "required": [
        "Title",
        "Year",
        "Rated",
        "Released",
        "Runtime",
        "Genre",
        "Director",
        "Writer",
        "Actors",
        "Plot",
        "Language",
        "Country",
        "Awards",
        "Poster",
        "Ratings",
        "Metascore",
        "imdbRating",
        "imdbVotes",
        "imdbID",
        "Type",
        "DVD",
        "BoxOffice",
        "Production",
        "Website",
        "Response"
      ],
      "type": "object"
    },
    "FfprobeFormat": {
      "additionalProperties": false,
