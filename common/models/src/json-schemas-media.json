    "LeveledLogEntry<any>": {
      "additionalProperties": false,
      "description": "Interface that represents a log entry with a specific level",
      "properties": {
        "data": {
          "description": "Additional entry data"
        },
        "level": {
          "$ref": "#/definitions/LogLevel",
          "description": "The verbosity level of the log entry"
        },
        "message": {
          "description": "The message string",
          "type": "string"
        },
        "scope": {
          "description": "A well-defined scope for grouping entries, e.g. a component or service name.",
          "type": "string"
        }
      },
      "required": [
        "level",
        "message",
        "scope"
      ],
      "type": "object"
    },
    "LogEntry<any>": {
      "additionalProperties": false,
      "description": "A log entry representation",
      "properties": {
        "data": {
          "description": "Additional entry data"
        },
        "message": {
          "description": "The message string",
          "type": "string"
        },
        "scope": {
          "description": "A well-defined scope for grouping entries, e.g. a component or service name.",
          "type": "string"
        }
      },
      "required": [
        "scope",
        "message"
      ],
      "type": "object"
    },
    "LogLevel": {
      "description": "The verbosity level of a log entry",
      "enum": [
        0,
        1,
        2,
        3,
        4,
        5
      ],
      "type": "number"
    },
    "Movie": {
      "additionalProperties": false,
      "properties": {
        "_id": {
          "type": "string"
        },
        "availableFormats": {
          "items": {
            "additionalProperties": false,
            "properties": {
              "codec": {
                "enum": [
                  "libvpx-vp9",
                  "x264"
                ],
                "type": "string"
              },
              "mode": {
                "const": "dash",
                "type": "string"
              }
            },
            "required": [
              "codec",
              "mode"
            ],
            "type": "object"
          },
          "type": "array"
        },
        "ffprobe": {
          "$ref": "#/definitions/FfprobeMetadata"
        },
        "libraryId": {
          "type": "string"
        },
        "metadata": {
          "$ref": "#/definitions/MovieUniversalMetadata"
        },
        "omdbMeta": {
          "$ref": "#/definitions/OmdbMetadata"
        },
        "path": {
          "type": "string"
        }
      },
      "required": [
        "_id",
        "path",
        "libraryId",
        "ffprobe",
        "metadata"
      ],
      "type": "object"
    },
    "MovieLibrary": {
      "additionalProperties": false,
      "properties": {
        "_id": {
          "type": "string"
        },
        "encoding": {
          "anyOf": [
            {
              "$ref": "#/definitions/EncodingType"
            },
            {
              "const": false,
              "type": "boolean"
            }
          ]
        },
        "icon": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "owner": {
          "$ref": "#/definitions/Owner"
        },
        "path": {
          "type": "string"
        }
      },
      "required": [
        "_id",
        "icon",
        "name",
        "path",
        "owner",
        "encoding"
      ],
      "type": "object"
    },
    "MovieUniversalMetadata": {
      "additionalProperties": false,
      "properties": {
        "duration": {
          "type": "number"
        },
        "episode": {
          "type": "number"
        },
        "genre": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "plot": {
          "type": "string"
        },
        "season": {
          "type": "number"
        },
        "seriesId": {
          "type": "string"
        },
