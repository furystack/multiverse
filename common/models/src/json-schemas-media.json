        "type": {
          "enum": [
            "movie",
            "episode"
          ],
          "type": "string"
        },
        "year": {
          "type": "number"
        }
      },
      "required": [
        "title",
        "genre",
        "thumbnailImageUrl",
        "plot",
        "type"
      ],
      "type": "object"
    },
    "MovieWatchHistoryEntry": {
      "additionalProperties": false,
      "properties": {
        "_id": {
          "type": "string"
        },
        "completed": {
          "type": "boolean"
        },
        "lastWatchDate": {
          "format": "date-time",
          "type": "string"
        },
        "movieId": {
          "type": "string"
        },
        "startDate": {
          "format": "date-time",
          "type": "string"
        },
        "userId": {
          "type": "string"
        },
        "watchedSeconds": {
          "type": "number"
        }
      },
      "required": [
        "_id",
        "userId",
        "movieId",
        "startDate",
        "lastWatchDate",
        "watchedSeconds",
        "completed"
      ],
      "type": "object"
    },
    "OmdbMetadata": {
      "anyOf": [
        {
          "$ref": "#/definitions/FailedOmdbMetadata"
        },
        {
          "$ref": "#/definitions/FetchedOmdbMetadata"
        }
      ]
    },
    "Owner": {
      "anyOf": [
        {
          "additionalProperties": false,
          "properties": {
            "type": {
              "const": "user",
              "type": "string"
            },
            "username": {
              "type": "string"
            }
          },
          "required": [
            "type",
            "username"
          ],
          "type": "object"
        },
        {
          "additionalProperties": false,
          "properties": {
            "organizationName": {
              "type": "string"
            },
            "type": {
              "const": "organization",
              "type": "string"
            }
          },
          "required": [
            "type",
            "organizationName"
          ],
          "type": "object"
        },
        {
          "additionalProperties": false,
          "properties": {
            "type": {
              "const": "system",
              "type": "string"
            }
          },
          "required": [
            "type"
          ],
          "type": "object"
        }
      ]
    },
    "Vp9EncodingFormat": {
      "additionalProperties": false,
      "properties": {
        "bitrate": {
          "additionalProperties": false,
          "properties": {
            "max": {
              "type": "number"
            },
            "min": {
              "type": "number"
            },
            "target": {
              "type": "number"
            }
          },
          "type": "object"
        },
        "downScale": {
          "type": "number"
        },
        "quality": {
          "type": "number"
        }
      },
      "type": "object"
    },
    "Vp9EncodingType": {
      "additionalProperties": false,
      "properties": {
        "codec": {
          "const": "libvpx-vp9",
          "type": "string"
        },
        "formats": {
          "items": {
            "$ref": "#/definitions/Vp9EncodingFormat"
          },
          "type": "array"
        },
        "mode": {
          "const": "dash",
          "type": "string"
        }
      },
      "required": [
        "mode",
        "codec",
        "formats"
      ],
      "type": "object"
    },
    "X264EncodingFormat": {
      "additionalProperties": false,
      "properties": {
        "bitRate": {
          "type": "number"
        },
        "downScale": {
          "type": "number"
        }
      },
      "required": [
        "downScale",
        "bitRate"
      ],
      "type": "object"
    },
    "X264EncodingType": {
      "additionalProperties": false,
      "properties": {
        "codec": {
          "const": "x264",
          "type": "string"
        },
        "formats": {
          "items": {
            "$ref": "#/definitions/X264EncodingFormat"
          },
          "type": "array"
        },
        "mode": {
          "const": "dash",
          "type": "string"
        }
      },
      "required": [
        "mode",
        "codec",
        "formats"
      ],
      "type": "object"
    }
  }
}