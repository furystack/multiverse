          "type": "string"
        },
        "nb_programs": {
          "type": "number"
        },
        "nb_streams": {
          "type": "number"
        },
        "probe_score": {
          "type": "number"
        },
        "size": {
          "type": "string"
        },
        "start_time": {
          "type": "string"
        },
        "tags": {
          "additionalProperties": {
            "type": "string"
          },
          "type": "object"
        }
      },
      "required": [
        "filename",
        "nb_streams",
        "nb_programs",
        "format_name",
        "format_long_name",
        "start_time",
        "duration",
        "size",
